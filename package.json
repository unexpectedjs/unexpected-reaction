{
  "name": "unexpected-reaction",
  "version": "2.5.2",
  "description": "An Unexpected plugin to make React testing with unexpected-dom more convenient",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --out-dir lib --quiet src",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "jest && npm run test:documentation",
    "test:documentation": "mocha documentation",
    "generate-site": "generate-site --require ./test/bootstrap-unexpected-markdown.js",
    "update-examples": "generate-site --require ./test/bootstrap-unexpected-markdown.js --update-examples",
    "deploy-site": "deploy-site.sh",
    "ci": "npm run lint && npm test"
  },
  "bugs": "https://github.com/unexpectedjs/unexpected-reaction",
  "repository": {
    "type": "git",
    "url": "https://github.com/unexpectedjs/unexpected-reaction"
  },
  "keywords": [
    "unexpected",
    "plugin",
    "react",
    "testing",
    "DOM"
  ],
  "author": "Sune Simonsen <sune@we-knowhow.dk>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "5.8.0",
    "eslint-config-pretty-standard": "2.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.0",
    "jest": "22.4.2",
    "jsdom": "11.7.0",
    "mocha": "5.0.5",
    "prettier": "1.11.1",
    "prop-types": "15.6.1",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-dom-testing": "1.5.0",
    "serve": "10.0.1",
    "sinon": "4.5.0",
    "unexpected": "10.37.5",
    "unexpected-documentation-site-generator": "4.7.1",
    "unexpected-dom": "4.9.4",
    "unexpected-markdown": "1.7.4",
    "unexpected-sinon": "10.9.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-dom-testing": "^1.2.0"
  }
}
